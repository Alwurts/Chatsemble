{
	"name": "chatsemble",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "biome check --write src",
		"typecheck": "tsc --noEmit",
		"preview": "vite preview",
		"deploy": "wrangler deploy",
		"cf-types": "wrangler types",
		"reset": "rm -rf .wrangler && npm run db:migrate:dev && npm run db:seed:dev",
		"db:generate": "drizzle-kit generate",
		"db:generate:organization": "DRIZZLE_SCHEMA=./src/server/organization-do/db/schema.ts DRIZZLE_OUT=./src/server/organization-do/db/migrations drizzle-kit generate --config=drizzle.do.config.ts",
		"db:migrate:dev": "wrangler d1 migrations apply chatsemble-db --local",
		"db:migrate:prod": "wrangler d1 migrations apply chatsemble-db --remote",
		"db:seed:dev": "wrangler d1 execute chatsemble-db --file=src/server/db/seed.sql --local",
		"db:seed:prod": "wrangler d1 execute chatsemble-db --file=src/server/db/seed.sql --remote",
		"db:studio": "drizzle-kit studio"
	},
