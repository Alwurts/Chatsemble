{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "do-chat-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-04",
  "migrations": [
    {
      "new_sqlite_classes": [
        "ChatDurableObject"
      ],
      "tag": "v1"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "CHAT_DURABLE_OBJECT",
        "class_name": "ChatDurableObject"
      }
    ]
  },
  "observability": {
    "enabled": true
  }, 
  "rules": [
    {
      "type": "Text",
      "globs": ["**/*.sql"],
      "fallthrough": true
    }
  ]
}
